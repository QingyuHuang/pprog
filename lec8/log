set term pdf
set title "Orbital motion"
set output "plot.pdf"
set key out
set size ratio -1
set grid
set xlabel "x"
set ylabel "y"
plot \
 "out_orbit.txt" using (1/$2) * sin($1):(1/$2) * cos($1) with lines notitle
